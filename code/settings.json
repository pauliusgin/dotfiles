  //* ======================= [TEXT COLORS] ==================================
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          //* comment green
          "punctuation.definition.comment.ts",
          "comment.line.double-slash.ts",
          "comment.line.number-sign.git-commit",
          "comment.line.number-sign.ini",
          "punctuation.definition.comment.ini",
          "punctuation.definition.comment.sql",
          "comment.line.double-dash.sql",
          "comment.line.double-slash.js",
          "punctuation.definition.comment.json"
        ],
        "settings": {
          "foreground": "#6A9955"
        }
      },
      {
        "scope": [
          //* string red
          "string.quoted.single.ts",
          "string.quoted.double.ts",
          "string.template.ts",
          "string.quoted.double.json.comments",
          "punctuation.definition.string.template.begin.ts",
          "punctuation.definition.string.template.end.ts",
          "string.quoted.single.sql",
          "string.quoted.single.yaml",
          "punctuation.definition.string.begin.yaml"
        ],
        "settings": {
          "foreground": "#CE9178"
        }
      },
      {
        "scope": [
          //* markdown blue
          "punctuation.definition.list.begin.markdown",
          "punctuation.definition.heading.markdown",
          "punctuation.definition.bold.markdown",
          "entity.name.section.markdown",
          "markup.bold.markdown",
          "keyword.other.definition.ini",
          "keyword.other.DML.sql",
          "keyword.other.DDL.create.II.sql",
          "keyword.other.sql"
        ],
        "settings": {
          "foreground": "#6796E6"
        }
      },
      {
        "scope": [
          "meta.object-literal.key.ts",
          "keyword.control.flow.ts",
          "storage.modifier.async.ts"
        ],
        "settings": {
          "foreground": "#FAFAFA"
        }
      },
      {
        "scope": [
          "variable.other.object.ts",
          "support.type.primitive.ts",
          "entity.other.inherited-class.ts",
          "variable.language.relations.prisma"
        ],
        "settings": {
          "foreground": "#9DA9B1"
        }
      },
      {
        "scope": [
          "constant.other.table-name.sql",
          "constant.other.database-name.sql",
          "meta.embedded.block.sql"
        ],
        "settings": {
          "foreground": "#BBBBBB"
        }
      },
      {
        "scope": [
          "entity.name.function.ts",
          "entity.name.type.ts",
          "punctuation.definition.string.end.sql",
          "punctuation.definition.string.begin.sql"
        ],
        "settings": {
          "foreground": "#E7A74D"
        }
      }
    ]
  },

  //* ========================= [BACKGROUND] =================================
  "background.enabled": true,
