  //* ============================ [VIM] =====================================
  "vim.leader": "<Space>",
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.easymotion": false,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [],
  "vim.commandLineModeKeyBindingsNonRecursive": [],
  "vim.visualModeKeyBindingsNonRecursive": [],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["c", "i", "e"],
      "commands": []
    },
    {
      "before": ["n"],
      "after": ["n", "z", "z"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z"]
    },
    {
      "before": ["{"],
      "after": ["{", "z", "z"]
    },
    {
      "before": ["}"],
      "after": ["}", "z", "z"]
    },
    {
      "before": ["("],
      "after": ["(", "z", "z"]
    },
    {
      "before": [")"],
      "after": [")", "z", "z"]
    },
    {
      "before": ["<C-u>"],
      "after": ["2", "0", "k", "z", "z"]
    },
    {
      "before": ["<C-d>"],
      "after": ["2", "0", "j", "z", "z"]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": []
    },
    {
      "before": ["d", "a", "e"],
      "commands": []
    },
    {
      "before": ["g", "b"],
      "commands": []
    },
    {
      "before": ["p"],
      "after": ["g", "p", "k", "w"]
    },
    {
      "before": ["P"],
      "after": ["g", "P", "k", "w"]
    },
    {
      "before": ["<C-End>"],
      "commands": [":noh"]
    }
  ],

  //* ============================ [GIT] =====================================
  "diffEditor.hideUnchangedRegions.enabled": false,
