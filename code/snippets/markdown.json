{
    "api documentation": {
        "prefix": "api",
        "body": [
            "## ${1|POST,PATCH,PUT,GET,DELETE|} /${2:endpoint}",
            "",
            "- ### Description:",
            "",
            "    $3",
            "",
            "- ### Constraints:",
            "",
            "    ${4|None, |}",
            "",
            "- ### Authorization:",
            "",
            "    ${5|Bearer token,None.|}",
            "",
            "- ### Query Params:",
            "",
            "    ${6|None, |}",
            "",
            "- ### Types:",
            "",
            "${7|    None.,```typescript\n{\n\ttypes\n}\n```|}",
            "",
            "- ### Request Body:",
            "",
            "${8|    None.,```json\n{\n\tbody\n}\n```|}",
            "",
            "- ### Response:",
            "",
            "${9|    OK (200),```json\n{\n\tresponse\n}\n```|}",
            "",
            "---"
        ]
    },

